<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Database.UserEntity" table="user" schema="homework">
        <id name="uaccount">
            <column name="u_account" sql-type="varchar(10)" length="10"/>
        </id>
        <property name="upwd">
            <column name="u_pwd" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="uname">
            <column name="u_name" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="urole">
            <column name="u_role" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="ustate">
            <column name="u_state" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>